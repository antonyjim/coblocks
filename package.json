{
	"name": "coblocks",
	"title": "CoBlocks",
	"description": "CoBlocks is a suite of professional page building blocks for the WordPress Gutenberg block editor.",
	"version": "1.17.3",
	"tested_up_to": "5.3",
	"author": "GoDaddy",
	"license": "GPL-2.0",
	"repository": "godaddy-wordpress/coblocks",
	"homepage": "https://wordpress.org/plugins/coblocks/",
	"bugs": {
		"url": "https://github.com/godaddy-wordpress/coblocks/issues"
	},
	"scripts": {
		"start": "wp-scripts start --config .dev/config/webpack.config.js ./src/blocks.js --output-path dist/ --output-filename blocks.build.js --progress",
		"build": "wp-scripts build --config .dev/config/webpack.config.js ./src/blocks.js --output-path dist/ --output-filename blocks.build.js --progress",
		"version": "grunt version && npm run babel:build && git add -A .",
		"postversion": "git push && git push --tags",
		"makepot": "wp i18n make-pot . --merge=languages/coblocks-js.pot --skip-audit --exclude=build,dist --headers='{\"Last-Translator\":\"plugins@godaddy.com\",\"Report-Msgid-Bugs-To\":\"https://github.com/godaddy-wordpress/coblocks/issues\"}' --file-comment='Copyright (c) 2019 GoDaddy Operating Company, LLC. All Rights Reserved.' && rm -f languages/coblocks-js.pot && grunt replace:languages && npm run pot2json",
		"babel": "babel src --ignore src/blocks/gist/components/gist.js",
		"babel:json": "wp i18n make-json languages",
		"babel:build": "npm run babel && npm run makepot",
		"test": "jest --config .dev/tests/jest/jest.config.js --silent --noStackTrace",
		"pot2json": ".dev/bin/pot2json languages/coblocks.pot languages/coblocks.json",
		"json2po": "cd languages && find . -name \\*.json ! -name 'coblocks*.json' -execdir sh -c '../.dev/bin/json2po coblocks.json \"$0\" coblocks-`basename $0 .json`.po' '{}' \\;",
		"po2mo": "cd languages && find . -name \\*.po -execdir sh -c 'msgfmt \"$0\" -o `basename $0 .po`.mo' '{}' \\;",
		"po2jed": "cd languages && find . -name \\*.po -execdir sh -c 'po2json \"$0\" `basename $0 .po`-coblocks-editor.json -f jed' '{}' \\;",
		"deploy-i18n": "./node_modules/.bin/cypress run -s './.dev/tests/cypress/integration/deploy-translations.js'",
		"test-e2e": "cypress run --config video=false --browser chrome",
		"test-e2e:record": "cypress run --browser chrome"
	},
	"locales": {
		"da_DK": "Dansk",
		"de_DE": "Deutsch",
		"el": "Ελληνικά",
		"es_ES": "Español",
		"fi": "Suomi",
		"fr_FR": "Français",
		"hi_IN": "हिन्दी",
		"id_ID": "Bahasa Indonesia",
		"it_IT": "Italiano",
		"ja": "日本語",
		"ko_KR": "한국어",
		"mr": "मराठी",
		"nb_NO": "Norsk bokmål",
		"nl_NL": "Nederlands",
		"pl_PL": "Polski",
		"pt_BR": "Português do Brasil",
		"pt_PT": "Português",
		"ru_RU": "Русский",
		"sv_SE": "Svenska",
		"th": "ไทย",
		"tr_TR": "Türkçe",
		"uk": "Українська",
		"vi": "Tiếng Việt",
		"zh_CN": "简体中文",
		"zh_TW": "繁體中文"
	},
	"devDependencies": {
		"@wordpress/babel-plugin-makepot": "^3.3.0",
		"@wordpress/scripts": "^6.0.0",
		"classnames": "^2.2.5",
		"css-loader": "^3.2.1",
		"email-validator": "^2.0.4",
		"memize": "^1.0.5",
		"node-sass": "^4.13.0",
		"react-flickity-component": "^3.4.0",
		"react-google-maps": "^9.4.5",
		"react-masonry-component": "^6.2.1",
		"react-slick": "^0.25.2",
		"recompose": "^0.30.0",
		"sass-loader": "^8.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint",
			"jest --config .dev/tests/jest/jest.config.js --silent --findRelatedTests"
		]
	},
	"stylelint": {
		"extends": [
			"stylelint-config-wordpress/scss",
			"stylelint-config-prettier"
		],
		"plugins": [
			"stylelint-order"
		],
		"rules": {
			"at-rule-empty-line-before": null,
			"no-descending-specificity": null,
			"order/properties-alphabetical-order": true
		}
	},
	"eslintConfig": {
		"extends": "wordpress"
	},
	"jest": {
		"collectCoverage": true,
		"coverageReporters": [
			"html"
		]
	}
}
